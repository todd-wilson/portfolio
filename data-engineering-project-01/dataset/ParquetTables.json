{
	"name": "ParquetTables",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SchemanName": {
				"type": "string"
			},
			"TableName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{concat(dataset().TableName, '.parquet')}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@ {concat(dataset().SchemanName, '/', dataset().TableName)}",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"compressionCodec": "none"
		},
		"schema": []
	}
}