{
	"name": "copy_pl",
	"properties": {
		"description": "Copies on-prem SQL Server tables to adls gen2.",
		"activities": [
			{
				"name": "Copy tables",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SqlServer_address",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}